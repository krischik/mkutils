@ECHO OFF

SETLOCAL
    PLUGIN /L pentium4-Release\lib\ada_demo.dll
    PLUGIN /P

    DIR
    REMARK
    USEBUFFER 123456789
    ECHO Reverse ABCDE is: %@Reverse[ABCDE]
    ECHO Hello is: %_Hello
    
    PLUGIN /U Ada_Demo
ENDLOCAL

